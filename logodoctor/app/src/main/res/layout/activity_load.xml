<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:oak="http://schemas.android.com/apk/res-auto"
    >

    <com.sollian.ld.views.svgview.AnimatedSvgView
        android:id="@+id/animated_svg_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        oak:oakSvgFillStart="1200"
        oak:oakSvgFillTime="1000"
        oak:oakSvgImageSizeX="410"
        oak:oakSvgImageSizeY="410"
        oak:oakSvgTraceTime="2000"
        oak:oakSvgTraceTimePerGlyph="1500"/>

    <TextView
        android:id="@+id/version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:layout_marginBottom="10dp"
        android:hint="version"
        android:textColor="@android:color/black"/>
</merge>